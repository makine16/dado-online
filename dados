#include <stdio.h>
#include <time.h>
#include <stdlib.h>

main(){
	int x,y,z;
	
	printf("Tipo de dado: ");
	//armazena a entrada de z
	scanf("%d",&z);
	
	printf("Quantidade de dados: ");
	//armazena a entrada de y
	scanf("%d",&y);
	
	//deixa sempre os números "aleatórios"
	srand((unsigned)time(NULL));
	
	//(inicialização;condição; incremento)
	for(x= 1 ;x <=y; x++)
	
	// rand() % z dá o limite do dado
	printf("Numero %d: %d\n", x, 1 + rand() % z );
};
